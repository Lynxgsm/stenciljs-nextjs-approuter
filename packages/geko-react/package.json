{
  "name": "geko-react",
  "version": "0.0.1",
  "author": "Lynxgsm <lynxgsm@github.com>",
  "license": "ISC",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./server": {
      "import": "./dist/esm/lib/server.js",
      "require": "./dist/cjs/lib/server.js",
      "types": "./dist/types/lib/server.d.ts"
    },
    "./client": {
      "import": "./dist/esm/lib/client/index.js",
      "require": "./dist/cjs/lib/client/index.js",
      "types": "./dist/types/lib/client/index.d.ts"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "patches"
  ],
  "scripts": {
    "prebuild": "del-cli --force ./dist && copyfiles -f ../geko-core/dist/components/*.js ./dist/esm/components && make-dir ./dist/esm && make-dir ./dist/types",
    "build": "npm run build:esm && npm run build:types && npm run build:cjs",
    "build:esm": "tsx ./scripts/build.ts",
    "build:types": "tsc -p .",
    "build:cjs": "esbuild dist/esm/*.js dist/esm/**/*.js dist/esm/**/**/*.js --log-level=error --outdir=dist/cjs --format=cjs",
    "lint": "eslint ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@stencil/core": "github:smartive/stencil-patched#v4.8.1",
    "geko-core": "^0.0.1",
    "linkedom": "^0.16.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.23.0",
    "@types/node": "20.11.10",
    "copyfiles": "2.4.1",
    "del-cli": "5.1.0",
    "esbuild": "0.20.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "make-dir-cli": "3.1.0",
    "prettier": "3.1.1",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  }
}
